{
	"welcomeFile": "index.html",
	"logout": {
		"logoutEndpoint": "/do/logout"
	},
	"authenticationMethod":"route",
	"routes": [
	{
		"source": "^/hello",
		"destination": "app-destination",
		"target": "/hello"
	}, 
	{
		"source": "^/debug",
		"destination": "app-destination",
		"target": "/debug"
	}, 	
    {
		"source": "^/(.*)",
		"localDir": "resources"
	}]
}